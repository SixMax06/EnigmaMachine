         </children></VBox>
      <VBox fx:id="vbxKeyboard" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #522702; -fx-border-color: #FFDF00; -fx-border-width: 5;">
         <children>
            <HBox fx:id="hbxKeyboard1" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" />
            <HBox fx:id="hbxKeyboard2" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" />
            <HBox fx:id="hbxKeyboard3" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" />
         </children>
      </VBox>
   </children>
</VBox>
