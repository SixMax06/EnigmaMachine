                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ComboBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="265.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefWidth="100.0" style="-fx-background-color: #ad7340;" text="Rotor 3" textFill="WHITE">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnIncreaseRotor3" mnemonicParsing="false" onAction="#IncreaseRotor3OnClick" style="-fx-background-color: #C0C0C0;" text="▲" />
