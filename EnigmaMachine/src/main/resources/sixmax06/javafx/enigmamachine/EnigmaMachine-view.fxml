            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefWidth="100.0" style="-fx-background-color: #ad7340;" text="Rotor 1" textFill="WHITE">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnIncreaseRotor1" mnemonicParsing="false" onAction="#IncreaseRotor1OnClick" style="-fx-background-color: #C0C0C0;" text="▲" />
                  <Label fx:id="lblPositionRotor1" alignment="CENTER" prefWidth="50.0" style="-fx-background-color: #ad7340;" text="A" textFill="WHITE">
                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnDecreaseRotor1" mnemonicParsing="false" onAction="#DecreaseRotor1OnClick" style="-fx-background-color: #C0C0C0;" text="▼" />
                  <ComboBox fx:id="cbxTypeRotor1" prefWidth="150.0" style="-fx-background-color: #C0C0C0;">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ComboBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="190.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
