                     <font>
                        <Font size="30.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnDecreaseRotor3" mnemonicParsing="false" onAction="#DecreaseRotor3OnClick" style="-fx-background-color: #C0C0C0;" text="▼" />
                  <ComboBox fx:id="cbxTypeRotor3" prefWidth="150.0" style="-fx-background-color: #C0C0C0;">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </ComboBox>
               </children>
            </VBox>
         </children></HBox>
      <VBox fx:id="vbxLampboard" layoutX="30.0" layoutY="398.0" prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #522702; -fx-border-color: #FFDF00; -fx-border-width: 5;">
         <children>
            <HBox fx:id="hbxLampboard1" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0" />
