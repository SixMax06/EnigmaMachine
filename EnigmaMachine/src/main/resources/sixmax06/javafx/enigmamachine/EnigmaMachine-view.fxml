                  <Label alignment="CENTER" prefWidth="100.0" style="-fx-background-color: #ad7340;" text="Rotor 2" textFill="WHITE">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <Button fx:id="btnIncreaseRotor2" mnemonicParsing="false" onAction="#IncreaseRotor2OnClick" style="-fx-background-color: #C0C0C0;" text="▲" />
