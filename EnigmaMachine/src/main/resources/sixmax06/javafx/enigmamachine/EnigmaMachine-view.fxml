<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="vbxEnigmaMachine" alignment="CENTER" prefHeight="800.0" prefWidth="700.0" spacing="25.0" style="-fx-background-color: #9e4a00;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sixmax06.javafx.enigmamachine.EnigmaMachineController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label alignment="CENTER" prefHeight="53.0" prefWidth="331.0" style="-fx-background-color: #ad7340;" text="Enigma Machine I" textFill="WHITE">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="230.0" spacing="30.0" style="-fx-background-color: #522702; -fx-border-color: #FFDF00; -fx-border-width: 5;">
